citHeader("To cite faux in publications use:")

citEntry(
  entry    = "Manual",
  title    = "Faux (v1.0)",
  author   = "Lisa DeBruine",
  doi      = "10.5281/zenodo.2669586",
  publisher = "Zenodo",
  year     = "2020",
  month    = "February",
  url      = "https://debruine.github.io/faux/",
  textVersion = paste("Lisa DeBruine (2020, February 29). Faux (v1.0). Zenodo. http://doi.org/10.5281/zenodo.2669586")
)
